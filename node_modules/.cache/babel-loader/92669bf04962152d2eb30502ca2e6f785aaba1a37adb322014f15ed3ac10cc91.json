{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// Importa Axios si deseas usarlo para realizar solicitudes HTTP\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      // Lista de tareas compartida\n      tasks: []\n    };\n  },\n  methods: {\n    // Método para agregar una tarea\n    addTask(newTask) {\n      this.tasks.push(newTask);\n    },\n    // Método para extraer tareas desde la API de DummyJSON\n    async fetchTasksFromAPI() {\n      try {\n        const response = await axios.get(\"https://dummyjson.com/todos\");\n        // Asignar las tareas obtenidas a la lista de tareas global\n        this.tasks = response.data.todos.map(todo => todo.title);\n      } catch (error) {\n        console.error(\"Error al obtener tareas de la API:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","tasks","methods","addTask","newTask","push","fetchTasksFromAPI","response","get","todos","map","todo","title","error","console"],"sources":["src/App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <!-- Barra de navegación con enlaces a diferentes rutas -->\r\n    <nav class=\"navbar navbar-expand-lg navbar-dark bg-dark\">\r\n      <div class=\"container\">\r\n        <router-link class=\"navbar-brand\" to=\"/\">Gestor de Tareas</router-link>\r\n        <button\r\n          class=\"navbar-toggler\"\r\n          type=\"button\"\r\n          data-bs-toggle=\"collapse\"\r\n          data-bs-target=\"#navbarNav\"\r\n          aria-controls=\"navbarNav\"\r\n          aria-expanded=\"false\"\r\n          aria-label=\"Toggle navigation\"\r\n        >\r\n          <span class=\"navbar-toggler-icon\"></span>\r\n        </button>\r\n        <div class=\"collapse navbar-collapse\" id=\"navbarNav\">\r\n          <ul class=\"navbar-nav ms-auto\">\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link\" to=\"/add-task\">Agregar Tarea</router-link>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link\" to=\"/fetch-tasks\">Extraer Tareas</router-link>\r\n            </li>\r\n            <li class=\"nav-item\">\r\n              <router-link class=\"nav-link\" to=\"/combined-view\">Vista Combinada</router-link>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </nav>\r\n\r\n    <!-- Componente dinámico de cada vista -->\r\n    <router-view \r\n      :tasks=\"tasks\"\r\n      @add-task=\"addTask\"\r\n      @fetch-tasks=\"fetchTasksFromAPI\"\r\n    />\r\n\r\n    <!-- Pie de página -->\r\n    <footer class=\"bg-dark text-white text-center py-3 mt-4\">\r\n      <p>&copy; {{ new Date().getFullYear() }} Gestor de Tareas - Todos los derechos reservados</p>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// Importa Axios si deseas usarlo para realizar solicitudes HTTP\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      // Lista de tareas compartida\r\n      tasks: [],\r\n    };\r\n  },\r\n  methods: {\r\n    // Método para agregar una tarea\r\n    addTask(newTask) {\r\n      this.tasks.push(newTask);\r\n    },\r\n\r\n    // Método para extraer tareas desde la API de DummyJSON\r\n    async fetchTasksFromAPI() {\r\n      try {\r\n        const response = await axios.get(\"https://dummyjson.com/todos\");\r\n        // Asignar las tareas obtenidas a la lista de tareas global\r\n        this.tasks = response.data.todos.map((todo) => todo.title);\r\n      } catch (error) {\r\n        console.error(\"Error al obtener tareas de la API:\", error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n@import 'bootstrap/dist/css/bootstrap.min.css';\r\n\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n}\r\n\r\n.navbar-nav .nav-link {\r\n  font-weight: bold;\r\n  color: #ffffff;\r\n}\r\n\r\n.navbar-nav .nav-link.router-link-exact-active {\r\n  color: #42b983;\r\n}\r\n\r\nfooter {\r\n  background-color: #343a40;\r\n  color: #ffffff;\r\n  padding: 1rem;\r\n  font-size: 0.9rem;\r\n}\r\n</style>\r\n"],"mappings":";AAgDA;AACA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACA;IACAC,QAAAC,OAAA;MACA,KAAAH,KAAA,CAAAI,IAAA,CAAAD,OAAA;IACA;IAEA;IACA,MAAAE,kBAAA;MACA;QACA,MAAAC,QAAA,SAAAR,KAAA,CAAAS,GAAA;QACA;QACA,KAAAP,KAAA,GAAAM,QAAA,CAAAP,IAAA,CAAAS,KAAA,CAAAC,GAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,KAAA;MACA,SAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,uCAAAA,KAAA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}